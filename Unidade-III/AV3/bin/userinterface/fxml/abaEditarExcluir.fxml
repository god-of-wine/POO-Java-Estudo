<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="178.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="userinterface.controller.abaEditarExcluirController">
   <children>
      <TextArea fx:id="infoPacoteArea" editable="false" prefHeight="178.0" prefWidth="300.0" />
      <Button fx:id="editarBotao" alignment="CENTER" defaultButton="true" layoutX="234.0" mnemonicParsing="false" onAction="#editarBotaoAcao" prefHeight="33.0" prefWidth="33.0">
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../imagens/icone/editar_icone.png" />
               </image>
            </ImageView>
         </graphic>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Button fx:id="deletarBotao" defaultButton="true" layoutX="267.0" mnemonicParsing="false" onAction="#deletarBotaoAcao" prefHeight="33.0" prefWidth="33.0">
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../imagens/icone/excluir_icone.png" />
               </image>
            </ImageView>
         </graphic>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
   </children>
</Pane>
